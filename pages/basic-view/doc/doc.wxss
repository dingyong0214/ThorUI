.container {
  padding-bottom: 80rpx;
}

.tui-banner {
  width: 100%;
  height: 375rpx;
}

.tui-text {
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  color: #b3b3b3;
  font-size: 26rpx;
  text-align: right;
}

.tui-view {
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}

.tui-cell {
  padding: 24rpx 0;
  color: #555;
}

.tui-title {
  width: 100%;
  padding: 0 8rpx;
  box-sizing: border-box;
}

.tui-link {
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
  background: #fff;
  box-shadow: 0px 3rpx 20rpx rgba(183, 183, 183, 0.1);
  border-radius: 10rpx;
  color: #06c;
  margin-top: 20rpx;
  word-break: break-all;
}
